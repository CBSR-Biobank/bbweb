{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": [ "app/assets/javascripts" ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "baseline": "./baseline-config.json",
    "cleverLinks": false,
    "monospaceLinks": false,
    "useLongnameInNav": false,
    "showInheritedInNav": true
  },
  "opts": {
    "template": "node_modules/jsdoc-baseline",
    "encoding": "utf8",
    "destination": "./documentation/js/",
    "recurse": true
  }
}
